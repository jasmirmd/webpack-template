{
	"$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"javascript": {
    "formatter": {
			"indentStyle": "space",
			"indentWidth": 2,
			"trailingCommas": "none",
      "quoteStyle": "single"
    }
  }
}
